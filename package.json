{
  "publisher": "skyler",
  "name": "ocrmnavigator",
  "displayName": "OCRMNavigator",
  "description": "",
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "git@github.com:8an3/OCRMNavigator.git"
  },
  "engines": {
    "vscode": "^1.98.0"
  },
  "$schema": "./navigator-config.schema.json",
  "categories": [],
  "activationEvents": [
    "onCommand:opinionatedCrm.showNavigator",
    "onCommand:opinionatedCrm.refreshNavigator",
    "onCommand:opinionatedCrm.editConfig",
    "onView:opinionatedCrmNavigator"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "opinionatedCrm.editConfig",
        "title": "Edit Navigator Config .json",
        "icon": "$(edit)"
      },
      {
        "command": "opinionatedCrm.revealInExplorer",
        "title": "Reveal in Explorer",
        "icon": "$(file-directory)"
      },
      {
        "command": "opinionatedCrm.copyPath",
        "title": "Copy Path",
        "icon": "$(clippy)"
      },
      {
        "command": "opinionatedCrm.addFileToNavigator",
        "title": "Add to CRM Navigator",
        "icon": "$(add)"
      },
      {
        "command": "opinionatedCrm.editFileLabel",
        "title": "Edit Label",
        "icon": "$(edit)"
      },
      {
        "command": "opinionatedCrm.deleteFile",
        "title": "Remove from Navigator",
        "icon": "$(trash)"
      },
      {
        "command": "opinionatedCrm.moveCategoryUp",
        "title": "Move Category Up",
        "icon": "$(arrow-up)"
      },
      {
        "command": "opinionatedCrm.moveCategoryDown",
        "title": "Move Category Down",
        "icon": "$(arrow-down)"
      },
      {
        "command": "opinionatedCrm.addCategory",
        "title": "Add Category",
        "icon": "$(add)"
      },
      {
        "command": "opinionatedCrm.renameCategory",
        "title": "Rename Category",
        "icon": "$(edit)"
      },
      {
        "command": "opinionatedCrm.deleteCategory",
        "title": "Remove Category",
        "icon": "$(trash)"
      },
      {
        "command": "opinionatedCrm.exportConfig",
        "title": "Export Config",
        "icon": "$(arrow-down)"
      },
      {
        "command": "opinionatedCrm.importConfig",
        "title": "Import Config",
        "icon": "$(arrow-up)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "opinionatedCrmNavigator",
          "name": "CRM Navigator",
          "icon": "$(list-tree)",
          "when": "workspaceFolderCount >= 1"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "opinionatedCrm.revealInExplorer",
          "when": "view == opinionatedCrmNavigator",
          "group": "navigation"
        },
        {
          "command": "opinionatedCrm.copyPath",
          "when": "view == opinionatedCrmNavigator",
          "group": "navigation"
        },
        {
          "command": "opinionatedCrm.editConfig",
          "when": "view == opinionatedCrmNavigator && viewItem == missing",
          "group": "navigation"
        },
        {
          "command": "opinionatedCrm.revealInExplorer",
          "when": "view == opinionatedCrmNavigator && viewItem != 'missing'",
          "group": "inline"
        },
        {
          "command": "opinionatedCrm.copyPath",
          "when": "view == opinionatedCrmNavigator",
          "group": "inline"
        },
        {
          "command": "opinionatedCrm.editFileLabel",
          "when": "view == opinionatedCrmNavigator && viewItem != 'category'",
          "group": "1_modification"
        },
        {
          "command": "opinionatedCrm.deleteFile",
          "when": "view == opinionatedCrmNavigator && viewItem != 'category'",
          "group": "1_modification"
        },
        {
          "command": "opinionatedCrm.moveCategoryUp",
          "when": "view == opinionatedCrmNavigator && viewItem == 'category'",
          "group": "2_ordering@1"
        },
        {
          "command": "opinionatedCrm.moveCategoryDown",
          "when": "view == opinionatedCrmNavigator && viewItem == 'category'",
          "group": "2_ordering@2"
        },
        {
          "command": "opinionatedCrm.addCategory",
          "when": "view == opinionatedCrmNavigator",
          "group": "1_modification@1"
        },
        {
          "command": "opinionatedCrm.renameCategory",
          "when": "view == opinionatedCrmNavigator && viewItem == 'category'",
          "group": "1_modification@1"
        },
        {
          "command": "opinionatedCrm.deleteCategory",
          "when": "view == opinionatedCrmNavigator && viewItem == 'category'",
          "group": "1_modification@2"
        }
      ],
      "explorer/context": [
        {
          "command": "opinionatedCrm.addFileToNavigator",
          "when": "resourceFilename",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "opinionatedCrm.editConfig",
          "when": "view == opinionatedCrmNavigator",
          "group": "navigation",
          "icon": "$(edit)"
        },
        {
          "command": "opinionatedCrm.exportConfig",
          "when": "view == opinionatedCrmNavigator",
          "group": "navigation@2",
          "icon": "$(export)"
        },
        {
          "command": "opinionatedCrm.importConfig",
          "when": "view == opinionatedCrmNavigator",
          "group": "navigation@2",
          "icon": "$(import)"
        },
        {
          "command": "opinionatedCrm.addCategory",
          "when": "view == opinionatedCrmNavigator",
          "group": "navigation@3",
          "icon": "$(add)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --no-dependencies",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}