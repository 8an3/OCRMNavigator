{
  "publisher": "skyler",
  "name": "ocrmnavigator",
  "displayName": "OCRMNavigator",
  "description": "",
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "git@github.com:8an3/OCRMNavigator.git"
  },
  "engines": {
    "vscode": "^1.98.0"
  },
  "$schema": "./navigator-config.schema.json",
  "categories": [],
  "activationEvents": [
    "onCommand:ocrmnavigator.showNavigator",
    "onCommand:ocrmnavigator.refreshNavigator",
    "onCommand:ocrmnavigator.editConfig",
    "onView:ocrmnavigatorNavigator"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ocrmnavigator",
          "title": "OCRM Navigator",
          "icon": "menu.svg" 
        }
      ]
    },
    "commands": [
      {
        "command": "ocrmnavigator.editConfig",
        "title": "Edit Navigator Config .json",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.revealInExplorer",
        "title": "Reveal in Explorer",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.copyPath",
        "title": "Copy Path",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.addFileToNavigator",
        "title": "Add to CRM Navigator",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.editFileLabel",
        "title": "Edit Label",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.deleteItem",
        "title": "Remove from Navigator",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.moveCategoryUp",
        "title": "Move Folder Up",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.moveCategoryDown",
        "title": "Move Folder Down",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.addCategory",
        "title": "Add Folder",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.renameCategory",
        "title": "Rename Folder",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.deleteCategory",
        "title": "Remove Folder",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.exportConfig",
        "title": "Export Config",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.importConfig",
        "title": "Import Config",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.addUrlToNavigator",
        "title": "Add Web URL",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.editWebUrl",
        "title": "Edit Web URL",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.executeCommand",
        "title": "Execute Command",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.addSubcategory",
        "title": "Add Subcategory",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.addCommandToCategory",
        "title": "Add Command",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.collapseFolder",
        "title": "Collapse Folder",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.expandFolder",
        "title": "Expand Folder",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.createSnippet",
        "title": "Add Snippet",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.editSnippet",
        "title": "Edit Snippet",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.deleteSnippet",
        "title": "Delete Snippet",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.refresh",
        "title": "Refresh Navigator",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.removeUrl",
        "title": "Remove Url",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.editCommand",
        "title": "Edit Command",
        "category": "OCRM Navigator"
      }  ,
      {
        "command": "ocrmnavigator.removeCommand",
        "title": "Remove Command",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.moveItem",
        "title": "Move Item",
        "category": "OCRM Navigator"
      },
      {
        "command": "ocrmnavigator.setMoveTarget",
        "title": "Select as Move Destination",
        "category": "OCRM Navigator"
      }
     
    ],
    "views": {
      "ocrmnavigator": [
        {
          "id": "ocrmnavigatorNavigator",
          "name": "Navigator",
          "icon": "menu.svg",
          "contextualTitle": "OCRM Navigator",
          "when": "workspaceFolderCount >= 1"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "ocrmnavigator.revealInExplorer",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation"
        },
        {
          "command": "ocrmnavigator.copyPath",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation"
        },
        {
          "command": "ocrmnavigator.revealInExplorer",
          "when": "view == ocrmnavigatorNavigator && viewItem != 'missing'",
          "group": "inline"
        },
        {
          "command": "ocrmnavigator.editFileLabel",
          "when": "view == ocrmnavigatorNavigator && viewItem != 'category'",
          "group": "1_modification"
        },
        {
          "command": "ocrmnavigator.deleteItem",
          "when": "view == ocrmnavigatorNavigator && viewItem != 'category' && viewItem != 'url'  && viewItem != 'command' ",
          "group": "1_modification"
        },
        {
          "command": "ocrmnavigator.editConfig",
          "when": "view == ocrmnavigatorNavigator && viewItem == missing",
          "group": "navigation"
        },
        {
          "command": "ocrmnavigator.moveCategoryUp",
          "when": "view == ocrmnavigatorNavigator && viewItem == 'category'",
          "group": "2_ordering@1"
        },
        {
          "command": "ocrmnavigator.moveCategoryDown",
          "when": "view == ocrmnavigatorNavigator && viewItem == 'category'",
          "group": "2_ordering@2"
        },
        {
          "command": "ocrmnavigator.addCategory",
          "when": "view == ocrmnavigatorNavigator",
          "group": "1_modification@1"
        },
        {
          "command": "ocrmnavigator.renameCategory",
          "when": "view == ocrmnavigatorNavigator && viewItem == 'category'",
          "group": "1_modification@1"
        },
        {
          "command": "ocrmnavigator.deleteCategory",
          "when": "view == ocrmnavigatorNavigator && viewItem == folder",
         "group": "navigation"
        },
         {
          "command": "ocrmnavigator.addSubcategory",
          "when": "view == ocrmnavigatorNavigator && viewItem == category",
          "group": "navigation"
        },
        {
          "command": "ocrmnavigator.addSubcategory",
          "when": "view == ocrmnavigatorNavigator && viewItem == allowed-category",
          "group": "inline"
        },
        {
          "command": "ocrmnavigator.expandFolder",
          "when": "view == ocmnavigatorView && viewItem == folder",
          "group": "navigation"
        },
        {
          "command": "ocrmnavigator.collapseFolder",
          "when": "view == ocmnavigatorView && viewItem == folder",
          "group": "navigation"
        },
        

        {
          "command": "ocrmnavigator.addCommandToCategory",
          "when": "view == ocrmnavigatorNavigator && viewItem == category",
          "group": "navigation"
        },
        {
          "command": "ocrmnavigator.editCommand",
          "when": "view == ocrmnavigatorNavigator && viewItem == command",
          "group": "navigation"
        },
        {
          "command": "ocrmnavigator.removeCommand",
          "when": "view == ocrmnavigatorNavigator && viewItem == command",
          "group": "navigation"
        },


        {
          "command": "ocrmnavigator.editWebUrl",
          "when": "view == ocrmnavigatorNavigator && viewItem == url",
          "group": "1_modification"
        },
        {
          "command": "ocrmnavigator.removeUrl",
          "when": "view == ocrmnavigatorNavigator && viewItem == 'url'",
          "category": "OCRM Navigator"
        } ,

        {
          "command": "ocrmnavigator.createSnippet",
          "when": "view == ocrmnavigatorNavigator && viewItem == category",
          "group": "navigation"
        },
        {
          "command": "ocrmnavigator.editSnippet",
          "when": "view == ocmnavigatorView && viewItem.label == '*.md'",
          "group": "snippets"
        },
        {
          "command": "ocrmnavigator.moveItem",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation@1"
        },
        {
          "command": "ocrmnavigator.setMoveTarget",
          "when": "view == ocrmnavigatorNavigator && viewItem == folder",
          "group": "navigation@2"
        }
     
      ],
      "explorer/context": [
        {
          "command": "ocrmnavigator.addFileToNavigator",
          "when": "resourceFilename",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "ocrmnavigator.addCategory",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation@3",
          "icon": "$(add)"
        },
        {
          "command": "ocrmnavigator.addUrlToNavigator",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation@3",
          "icon": "$(link)"
        },
        {
          "command": "ocrmnavigator.addCommandToCategory",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation@3",
          "icon": "$(terminal)"
        },
        {
          "command": "ocrmnavigator.createSnippet",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation@1"
        },
        {
          "command": "ocrmnavigator.exportConfig",
          "title": "Export Config",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation@1"
        },
        {
          "command": "ocrmnavigator.importConfig",
          "title": "Import Config",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation@1"
        },
        {
          "command": "ocrmnavigator.editConfig",
          "title": "Edit Config",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation@1"
        },
        {
          "command": "ocrmnavigator.refresh",
          "when": "view == ocrmnavigatorNavigator",
          "group": "navigation@3"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --no-dependencies",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}